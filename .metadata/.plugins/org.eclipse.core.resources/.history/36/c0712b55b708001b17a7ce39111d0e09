import java.util.Scanner;

//Addition Program. 
public class Addition {
	//main methods begin the execution of the program.
	public static void main( String[] args) {
		//Creates a scanner input from console.
		Scanner input = new Scanner(System.in);
		
		int number1; //First number.
		int number2; //Second number to add.
		int sum; //sum of both numbers.
		
		System.out.print("Enter First integer\n");
		number1 = input.nextInt();
		
		System.out.print("Enter Second integer\n");
		number2 = input.nextInt();
		
		sum = number1 + number2; //addition stored in sum int;
		
		System.out.printf("The Sum is %d\n, sum "); //displays sum.
	} //end method main
} //end of class.
